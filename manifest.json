{
    "manifest_version": 2,
    "name": "Auto Download Extension",
    "version": "0.1.0",
    "description": "auto download files in UMP",
    "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": [
          "",
          ""
        ],
        "js": [
          "content.js"
        ]
      },
      {
        "matches": [
          ""
        ],
        "js": [
          "/clickEvent/firstclick.js"
        ]
      },
      {
        "matches": [
          "",
          ""
        ],
        "js": [
          "/clickEvent/secondclick.js"
        ]
      },
      {
        "matches": [
          "",
          ""
        ],
        "js": [
          "/clickEvent/thirdclick.js"
        ]
      },
      {
        "matches": [
          "",
          ""
        ],
        "js": [
          "/clickEvent/fourthclick.js"
        ]
      },
      {
        "matches": [
          "",
          ""
        ],
        "js": [
          "download.js"
        ]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "tabs",
      "storage"
    ]
  }